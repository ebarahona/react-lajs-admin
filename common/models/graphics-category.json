{
  "name": "graphicsCategory",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "showInTable": false,
      "showInput": false
    },
    "name": {
      "type": "string",
      "required": true,
      "showInTable": true,
      "showInput": true,
      "sortable": true
    },
    "thumb": {
      "type": "string",
      "required": true,
      "showInTable": true,
      "showInput": true,
      "sortable": false,
      "representation": "THUMBNAIL",
      "inputElement": "FILE_INPUT",
      "uploadParams": {
        "dir": "files/graphicCategoryThumbs/",
        "endpoint": "containers/graphicCategoryThumbs"
      }
    },
    "graphicsCategoryId": {
      "type": "string",
      "required": false,
      "showInTable": false,
      "showInput": true,
      "header": "Parent category",
      "inputElement": "DATA_SELECT",
      "secondaryData": "graphicsCategories"
    }
  },
  "validations": [],
  "relations": {
    "category": {
      "type": "hasMany",
      "model": "graphicsCategory",
      "foreignKey": "graphicsCategoryId"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
