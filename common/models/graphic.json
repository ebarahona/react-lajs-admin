{
  "name": "graphic",
  "base": "PersistedModel",
  "idInjection": true,
  "forceId": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "categoryId": {
      "type": "string",
      "required": true
    },
    "colors": {
      "type": "string"
    },
    "colorize": {
      "type": "boolean",
      "default": false
    },
    "multicolor": {
      "type": "boolean",
      "default": false
    },
    "thumb": {
      "type": "string"
    },
    "image": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "colorizableElements": {
      "type": "embedsMany",
      "model": "colorizableElement",
      "property": "colorizables",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
