{
  "name": "graphic",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "showInTable": false,
      "showInput": false
    },
    "name": {
      "type": "string",
      "required": true,
      "showInTable": true,
      "showInput": true,
      "sortable": true
    },
    "description": {
      "type": "string",
      "showInTable": false,
      "showInput": true,
      "inputElement": "TEXTAREA"
    },
    "categoryId": {
      "type": "string",
      "required": true,
      "showInTable": true,
      "showInput": true,
      "sortable": true,
      "header": "Category",
      "textProperty": "name",
      "representation": "TEXT",
      "sortElement": "SELECT",
      "inputElement": "DATA_SELECT",
      "secondaryData": "graphicsCategories"
    },
    "colors": {
      "type": "array",
      "showInTable": false,
      "showInput": true,
      "inputElement": "DATA_MULTISELECT",
      "secondaryData": "colors",
      "secondaryDataId": "value"
    },
    "colorizables": {
      "type": [
        {
          "name": "string",
          "id": "string",
          "assignColorgroup": "boolean",
          "_colors": [
            {
              "name": "string",
              "value": "string"
            }
          ],
          "colorgroup": {
            "name": "string",
            "id": "string"
          }
        }
      ],
      "showInTable": false,
      "showInput": true
    },
    "colorize": {
      "type": "boolean",
      "default": false,
      "showInTable": false,
      "showInput": true,
      "inputElement": "BINARY_SELECT"
    },
    "multicolor": {
      "type": "boolean",
      "default": false,
      "showInTable": false,
      "showInput": true,
      "inputElement": "BINARY_SELECT"
    },
    "thumb": {
      "type": "string",
      "showInTable": true,
      "showInput": true,
      "sortable": false,
      "representation": "THUMBNAIL",
      "inputElement": "FILE_INPUT",
      "uploadParams": {
        "dir": "files/graphicThumbs/",
        "endpoint": "containers/graphicThumbs"
      }
    },
    "image": {
      "type": "string",
      "required": true,
      "showInTable": false,
      "showInput": true,
      "representation": "THUMBNAIL",
      "inputElement": "FILE_INPUT",
      "uploadParams": {
        "dir": "files/graphicImages/",
        "endpoint": "containers/graphicImages"
      }
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
