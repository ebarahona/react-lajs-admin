{
  "name": "product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "data": {
      "type": {
        "price": "string",
        "material": "string"
      },
      "default": {
        "price": "",
        "material": ""
      }
    },
    "categoryId": {
      "type": "string",
      "required": true
    },
    "minDPU": {
      "type": "number"
    },
    "minQuantity": {
      "type": "number"
    },
    "colorize": {
      "type": "boolean"
    },
    "multicolor": {
      "type": "boolean"
    },
    "hideEditableAreaBorder": {
      "type": "boolean"
    },
    "namesNumbersEnabled": {
      "type": "boolean"
    },
    "pantones": {
      "type": {
        "useForDecoration": "boolean",
        "useForProduct": "boolean"
      },
      "default": {
        "useForDecoration": false,
        "useForProduct": false
      },
      "required": false
    },
    "resizable": {
      "type": "boolean",
      "default": false
    },
    "editableAreaSizes": {
      "type": [
        {
          "label": "string",
          "width": "number",
          "height": "number"
        }
      ],
      "required": false
    },
    "showRuler": {
      "type": "boolean",
      "default": false
    },
    "sizes": {
      "type": [
        "string"
      ],
      "required": false
    },
    "locations": {
      "type": [
        {
          "name": {
            "type": "string",
            "required": true
          },
          "image": {
            "type": "string",
            "required": false
          },
          "mask": {
            "type": "string"
          },
          "overlayInfo": {
            "type": "string"
          },
          "editableArea": {
            "type": "array"
          },
          "editableAreaUnits": {
            "type": "array"
          },
          "editableAreaUnitsRange": {
            "type": "array"
          },
          "editableAreaUnitsRestrictRotation": {
            "type": "boolean",
            "default": false
          },
          "clipRect": {
            "type": "number"
          }
        }
      ]
    },
    "template": {
      "type": "string",
      "required": false
    },
    "thumbUrl": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "colorizableElements": {
      "type": "embedsMany",
      "model": "colorizableElement",
      "property": "colorizables",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "colors": {
      "type": "embedsMany",
      "model": "color-embed",
      "property": "_colors",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
