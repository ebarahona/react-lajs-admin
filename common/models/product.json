{
  "name": "product",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "data": {
      "type": {
        "price": "string",
        "material": "string"
      },
      "default": {
        "price": "",
        "material": ""
      },
      "required": false
    },
    "categoryId": {
      "type": "string",
      "required": true
    },
    "minDPU": {
      "type": "number",
      "required": false
    },
    "minQuantity": {
      "type": "number",
      "required": false
    },
    "colorize": {
      "type": "boolean",
      "default": false
    },
    "multicolor": {
      "type": "boolean",
      "default": false
    },
    "hideEditableAreaBorder": {
      "type": "boolean",
      "default": false
    },
    "namesNumbersEnabled": {
      "type": "boolean",
      "default": false
    },
    "pantones": {
      "type": {
        "useForDecoration": {
          "type": "boolean",
          "default": false
        },
        "useForProduct": {
          "type": "boolean",
          "default": false
        }
      },
      "required": false
    },
    "resizable": {
      "type": "boolean",
      "default": false
    },
    "editableAreaSizes": {
      "type": {
        "label": "string",
        "width": "number",
        "height": "number"
      },
      "required": false
    },
    "showRuler": {
      "type": "boolean",
      "default": false
    },
    "sizes": {
      "type": [
        "string"
      ],
      "required": false
    },
    "template": {
      "type": "string",
      "required": false
    },
    "thumbUrl": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "colorizableElements": {
      "type": "embedsMany",
      "model": "colorizableElement",
      "property": "colorizables",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "colors": {
      "type": "embedsMany",
      "model": "ColorEmbed",
      "property": "colors",
      "options": {
        "validate": true,
        "forceId": false
      }
    },
    "location": {
      "type": "embedsMany",
      "model": "location",
      "property": "locations",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [],
  "methods": {}
}
